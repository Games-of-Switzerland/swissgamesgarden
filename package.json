{
  "name": "gos-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "test": "jest --watch",
    "test:ci": "jest --ci"
  },
  "dependencies": {
    "@popperjs/core": "^2.5.2",
    "@svgr/webpack": "^5.4.0",
    "@tailwindcss/forms": "^0.3.3",
    "@tippyjs/react": "^4.2.0",
    "autoprefixer": "^10.3.2",
    "classnames": "^2.2.6",
    "downshift": "^6.0.6",
    "i18next": "^20.4.0",
    "kitsu": "^9.0.6",
    "kitsu-core": "^9.0.6",
    "konva": "^7.1.4",
    "next": "^11.1.0",
    "next-seo": "^4.15.0",
    "postcss": "^8.3.6",
    "postcss-import": "^14.0.2",
    "prop-types": "^15.7.2",
    "query-string": "^7.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.7.3",
    "react-konva": "^17.0.0-0",
    "react-markdown": "^7.0.1",
    "react-popper": "^2.2.3",
    "react-query": "^3.21.0",
    "react-ranger": "^2.1.0",
    "react-use": "^17.2.4",
    "tippy.js": "^6.2.7"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@types/node": "^14.14.0",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "postcss-cli": "^8.3.1",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-inline-svg": "^5.0.0",
    "postcss-nested": "^5.0.6",
    "prettier": "^2.0.5",
    "tailwindcss": "^2.2.7"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie < 11"
  ],
  "prettier": {
    "trailingComma": "es5",
    "bracketSpacing": false,
    "jsxBracketSameLine": false,
    "singleQuote": true,
    "arrowParens": "avoid"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts",
      "!**/node_modules/**"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setupTests.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/.next/"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    }
  },
  "babel": {
    "presets": [
      "next/babel"
    ],
    "env": {
      "test": {
        "plugins": [
          [
            "module-resolver",
            {
              "root": [
                "./"
              ],
              "alias": {
                "components": "./src/components"
              }
            }
          ]
        ]
      }
    }
  }
}
